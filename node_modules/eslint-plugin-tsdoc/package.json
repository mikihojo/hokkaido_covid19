	},
	"dependencies": {
		"@microsoft/tsdoc": "0.12.19",
		"@microsoft/tsdoc-config": "0.13.3"
	},
	"devDependencies": {
		"@rushstack/eslint-config": "0.4.0",
		"@types/eslint": "6.1.3",
		"@types/estree": "0.0.39",
		"@types/node": "10.17.5",
		"eslint": "^6.0.0",
		"rimraf": "~2.6.3",
		"typescript": "~3.5.3"
	}
}
